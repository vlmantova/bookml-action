{
  "problemMatcher": [
    {
      "owner": "bookml-latex-errors",
      "pattern": [
        {
          "regexp": "^([^:]*):([0-9]+):\\s*(.*)$",
          "file": 1,
          "line": 2,
          "message": 3
        }
      ]
    },
    {
      "owner": "bookml-latexml-errors",
      "pattern": [
        {
          "regexp": "^(Error|Fatal):([^:]+:.*) at ([^;]+);( from)?( line ([0-9]+))?( col ([0-9]+))?( to line [0-9]+( col [0-9]+)?)?$",
          "file": 3,
          "line": 6,
          "column": 8,
          "message": 2
        }
      ]
    },
    {
      "owner": "bookml-latexml-warnings",
      "severity": "warning",
      "pattern": [
        {
          "regexp": "^(Warn):([^:]+:.*) at ([^;]+);( from)?( line ([0-9]+))?( col ([0-9]+))?( to line [0-9]+( col [0-9]+)?)?$",
          "file": 3,
          "line": 6,
          "column": 8,
          "message": 2
        }
      ]
    }
  ]
}
